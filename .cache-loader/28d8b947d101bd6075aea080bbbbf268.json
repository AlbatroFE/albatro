        1\n      ),\n      _vm._v(\" \"),\n      _c(\n        \"al-container\",\n        [\n          _c(\"al-aside\", { attrs: { width: \"200px\" } }, [_vm._v(\"Aside\")]),\n          _vm._v(\" \"),\n          _c(\n            \"al-container\",\n            [\n              _c(\"al-header\", [_vm._v(\"Header\")]),\n              _vm._v(\" \"),\n              _c(\"al-main\", [_vm._v(\"Main\")])\n            ],\n            1\n          )\n        ],\n        1\n      ),\n      _vm._v(\" \"),\n      _c(\n        \"al-container\",\n        [\n          _c(\"al-aside\", { attrs: { width: \"200px\" } }, [_vm._v(\"Aside\")]),\n          _vm._v(\" \"),\n          _c(\n            \"al-container\",\n            [\n              _c(\"al-header\", [_vm._v(\"Header\")]),\n              _vm._v(\" \"),\n              _c(\"al-main\", [_vm._v(\"Main\")]),\n              _vm._v(\" \"),\n              _c(\"al-footer\", [_vm._v(\"Footer\")])\n            ],\n            1\n          )\n        ],\n        1\n      )\n    ],\n    1\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\n  \n    const democomponentExport = {}\n    return {\n      ...democomponentExport,\n      render,\n      staticRenderFns\n    }\n  })(),\n        }\n      }\n    </script>\n  "]}